{
  "name": "hubspot-revenue-leak-detection-engine",
  "version": "2.0.0",
  "description": "HubSpot Native App with UI Extensions for identifying and recovering hidden revenue leaks across the full customer lifecycle. Includes CRM cards, embedded dashboard, workflow actions, and integrations with Stripe, QuickBooks, Outlook, Gmail, Shopify, and Salesforce.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "leak-engine": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "cli": "ts-node src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "hubspot",
    "hubspot-app",
    "revenue",
    "leak-detection",
    "crm",
    "ui-extensions",
    "workflow-actions",
    "breeze-agent",
    "sales",
    "billing",
    "stripe",
    "quickbooks"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@hubspot/api-client": "^13.4.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
